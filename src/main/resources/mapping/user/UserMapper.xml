<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.car.user.mapper.UserMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.car.user.model.User">
        <id column="card_number" property="cardNumber" />
        <result column="pay_password" property="payPassword" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        card_number,pay_password
    </sql>

    
    <select id="carList" resultType="java.util.List">
        select * from car
    </select>
</mapper>
