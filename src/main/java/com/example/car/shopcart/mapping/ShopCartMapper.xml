<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.car.shopcart.mapper.ShopCartMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.car.shopcart.model.ShopCart">
        <id column="id" property="id" />
        <result column="user_id" property="userId" />
        <result column="car_id" property="carId" />
        <result column="car_count" property="carCount" />
        <result column="creat_time" property="creatTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, user_id, car_id, car_count, creat_time
    </sql>

</mapper>
